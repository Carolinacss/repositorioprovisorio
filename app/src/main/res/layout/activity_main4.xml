<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".MainActivity4">

    <TextView
        android:id="@+id/txtbatman"
        android:layout_width="119dp"
        android:layout_height="53dp"
        android:layout_marginStart="146dp"
        android:layout_marginTop="619dp"
        android:layout_marginEnd="146dp"
        android:layout_marginBottom="59dp"
        android:text="@string/txtbatman"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,TextSizeCheck" />

    <ImageButton
        android:id="@+id/btnaquaman"
        android:layout_width="154dp"
        android:layout_height="141dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="199dp"
        android:layout_marginEnd="125dp"
        android:layout_marginBottom="391dp"
        android:onClick="btnaquaman"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/aquaman2"
        tools:ignore="ContentDescription,MissingConstraints,ImageContrastCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/txtaquaman"
        android:layout_width="161dp"
        android:layout_height="48dp"
        android:layout_marginStart="131dp"
        android:layout_marginTop="361dp"
        android:layout_marginEnd="119dp"
        android:layout_marginBottom="322dp"
        android:text="@string/txtaquaman"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,TextSizeCheck" />

    <ImageButton
        android:id="@+id/btnbatman"
        android:layout_width="150dp"
        android:layout_height="142dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="450dp"
        android:layout_marginEnd="127dp"
        android:layout_marginBottom="135dp"
        android:onClick="btnbatman"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/batman2"
        tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/btnvoltar"
        android:layout_width="69dp"
        android:layout_height="47dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="326dp"
        android:layout_marginBottom="668dp"
        android:backgroundTint="@color/black"
        android:onClick="btnvoltar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_revert"
        tools:ignore="ContentDescription,MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imgdc"
        android:layout_width="135dp"
        android:layout_height="130dp"
        android:layout_marginStart="138dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="138dp"
        android:layout_marginBottom="576dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dclogo2"
        tools:ignore="ContentDescription,MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>